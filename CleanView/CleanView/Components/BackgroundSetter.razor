@code {
    [Parameter]
    public string Url { get; set; } = "_content/CleanView/background/background4.jpg";

    [CascadingParameter(Name = "CleanLayout")]
    public CleanLayout CleanLayout { get; set; }
        protected override void OnInitialized()
    {
        CleanLayout.SetBackground(Url);
        base.OnInitialized();
    }
}
